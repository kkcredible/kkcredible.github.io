    <script>
        var imageURLs = [];
        var videoURLs = [];
        for (let i = 1; i <= 66; i++) {
            imageURLs.push(i +'-image.jpg');
            videoURLs.push(i +'-video.mp4');
        }

        function getImageTag() {
            var img = '<div class="col-md-4"><div class="service-item second-row"><img src=\"images/loader.gif" data-image="images/downloads/images/';
            var randomIndex = Math.floor(Math.random() * imageURLs.length);
            img += imageURLs[randomIndex];
            img += '\" class="img-responsive async-img" width="240"></div></div>';
            return img;
        }

        function getVideoTag() {
            var videos = [
                'g_JfCm34adY', 'yI4sJ-f5fIA', 'WGdQNkGkFzU', 'qf7StPSIp9o', 'H2glTF3QmTA', 'wg-uzI1XfNc', 'atF8TrOJUWE', 'SdkpigbRMJg', 'kcsFdaV9Sfg', 'ynFmqVNDpBo', 'Cr9PcACmoz8', 'FNrpBzfb-JU', '0_Qpq27a7_8', 'g0TJusdPK2o', 'b6D__NoXe-w', 'ntEgX3UYoQs', 'cPNQYeBaYk4', '_qONyBvRpVI', 'A5HYToPDl6A', 'D14RjMIc3eM', 'CyqMt0DWqCk', '-EpvkuTEwdA', 'U-rhmew73og', 'y-6RKWTnAgw', 'I2V8TWmOB-0', 'fq4r66Sskzw', '2Ewz6cVNkS4', 'dOjvuzqEGiw', '9UJgxYOwoB8', 'GGasOSkVlEs', 'SEvk4D2dUAU', 'EEB39msgpxo', 'hfkh6PPqny4', 'JrJro8JvFkU', 'T8wYm1ErMGI', 'lyNzP2rT_dQ', 'E9D6JhlNyFM', 'tSocIPvlnyY', '_fjxraU5QFU', 'xVxatGtPYj4', 'aip3-8uE4iA', 'OkuvmhNxRVE', 'NSarOrysNbs', 'sN2RHVUQ5ds', 'SiV2BD77e38', 'svV5rBmjbms', 'JzQOUk6Hdms', 'W8k0MVH9dQE', 'j9qC2HHRVos', 'C9Mu_Z8pH5Y', 'FgQH74eVP4M', 'ZwEBZG1T3VA', '-m6nZ1jGaHg', 'OEUNsvFHYOE', 'qybpPZpBwI8', 'p25JQkpTFEI',
            ];

            var index = Math.floor(Math.random() * videos.length);
            var html = '<div id="video"><h3>Random Video</h3> </div>';

            var video = '<div class="col-xs-12 col-sm-6 col-md-4"><div class="embed-responsive embed-responsive-4by3"><iframe class="youtube-video embed-responsive-item" src="//www.youtube.com/embed/' + videos[index] + '?autoplay=1" frameborder="0" allowfullscreen></iframe></div></div>';

            return video;
        }

        window.onload = function() {
            for (let i = 0; i <= 5; i++) {
                var images = $('#images-container').html();

                $('#images-container').html(images + getImageTag());
                loadImages();
                var video = $('#video-container').html();
                $('#video-container').html(video + getVideoTag());
            }
        };
    </script>

